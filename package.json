{
  "name": "vantage-stories",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thometnanni/vantage-stories.git"
  },
  "main": "./dist/vantage-renderer.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/vantage-renderer.js",
      "require": "./dist/vantage-renderer.umd.cjs"
    }
  },
  "scripts": {
    "dev": "svelte-kit sync && vite dev",
    "dev:legacy": "vite --config vite.config.site.js",
    "build": "svelte-kit sync && vite build",
    "build:site": "npm run build",
    "build:lib": "vite build --config vite.config.lib.js",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist --dotfiles"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "svelte": "^5.16.0",
    "tailwindcss": "^3.4.17",
    "vite": "^5.4.1"
  },
  "dependencies": {
    "gh-pages": "^6.3.0",
    "jszip": "^3.10.1",
    "three": "^0.172.0",
    "three-projected-material": "github:fidelthomet/three-projected-material"
  }
}
